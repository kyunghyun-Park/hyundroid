        app:layout_constraintTop_toBottomOf="@+id/chungpook" />

    <TextView
        android:id="@+id/kyongpook"
        android:layout_width="44dp"
        android:layout_height="18dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="28dp"
        app:layout_constraintStart_toEndOf="@+id/chungnam"
        app:layout_constraintTop_toBottomOf="@+id/chungpook" />

    <TextView
        android:id="@+id/daegu"
        android:layout_width="28dp"
        android:layout_height="39dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="48dp"
        app:layout_constraintStart_toEndOf="@+id/daejun"
        app:layout_constraintTop_toBottomOf="@+id/kyongpook" />

    <TextView
        android:id="@+id/junpook"
        android:layout_width="49dp"
        android:layout_height="21dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="40dp"
        app:layout_constraintEnd_toStartOf="@+id/daegu"
        app:layout_constraintTop_toBottomOf="@+id/daejun" />

    <TextView
        android:id="@+id/ulsan"
        android:layout_width="27dp"
        android:layout_height="33dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="32dp"
        app:layout_constraintStart_toEndOf="@+id/daegu"
        app:layout_constraintTop_toTopOf="@+id/daegu" />

    <TextView
        android:id="@+id/pusan"
        android:layout_width="28dp"
        android:layout_height="30dp"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.699"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ulsan" />

    <TextView
        android:id="@+id/kangju"
        android:layout_width="31dp"
        android:layout_height="29dp"
        android:layout_marginTop="42dp"
